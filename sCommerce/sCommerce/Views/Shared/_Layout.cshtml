        <nav class="panel-menu mobile-main-menu">
            <ul>
                <li><a href="@Url.Action("Index")">Ana Sayfa</a></li>
                <li><a href="@Url.Action("Shop")">Mağaza</a></li>
                <li><a href="@Url.Action("Blog")">Blog</a></li>
                <li><a href="@Url.Action("Contact")">İletişim</a></li>
                <li><a href="@Url.Action("About")">Hakkımızda</a></li>
            </ul>
            <div class="mm-navbtn-names">
                <div class="mm-closebtn">Kapat</div>
                <div class="mm-backbtn">Geri</div>
            </div>
        </nav>
        <nav class="panel-menu mobile-caterorie-menu">
            <ul>
                @foreach (Kategori kategori in Site.urunKategorileri)
                {
                    <li>
                        <a href="@Url.Action("Category", new { id = kategori.kategoriID })">
                            @if (kategori.ikon.Length > 0)
                            {
                                <i class="@kategori.ikon"></i>
                            }
                            <span>@kategori.kategori</span>
                        </a>
                        @if (kategori.altKategori.Count > 0)
                        {
                        <ul>
                            @foreach (Kategori altKategori0 in kategori.altKategori)
                            {
                                <li>
                                    <a href="@Url.Action("Category", new { id = altKategori0.kategoriID })">
                                        @if (altKategori0.ikon.Length > 0)
                                        {
                                            <i class="@altKategori0.ikon"></i>
                                        }
                                        <span>@altKategori0.kategori</span>
                                    </a>
                                    @if (altKategori0.altKategori.Count > 0)
                                    {
                                        <ul>
                                            @foreach (Kategori altKategori1 in altKategori0.altKategori)
                                            {
                                                <li>
                                                    <a href="@Url.Action("Category", new { id = altKategori1.kategoriID })">
                                                        @if (altKategori1.ikon.Length > 0)
                                                        {
                                                            <i class="@altKategori1.ikon"></i>
                                                        }
                                                        <span>@altKategori1.kategori</span>
                                                    </a>
                                                </li>
                                            }
                                        </ul>
                                    }
                                </li>
                            }
                        </ul>
                        }
                    </li>
                }
            </ul>
            <div class="mm-navbtn-names">
                <div class="mm-closebtn">Kapat</div>
                <div class="mm-backbtn">Geri</div>
            </div>
